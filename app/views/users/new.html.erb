<h1> Sign up</h1>
<%= form_for @user do |f| %>
  <%= f.label "Email" %>
  <%= f.text_field :email %><br>
  <%= f.label "User Name" %>
  <%= f.text_field :user_name %><br>
  <%# <%= f.label :admin %>
  <%# <%= f.select :admin, [ 'true','false'], :prompt => 'Select One' %>
  <%= f.label "Password" %>
  <%= f.password_field :password %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %><br>
  <%= f.submit "Sign Up" %>
<% end %>